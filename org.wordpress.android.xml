<rules>
<service block="true" log="false">
    <component-filter name="org.wordpress.android/.networking.ConnectionChangeReceiver" />
    <component-filter name="org.wordpress.android/.ui.notifications.receivers.NotificationsPendingDraftsReceiver" />
    <component-filter name="org.wordpress.android/.ui.stats.StatsWidgetProvider" />
