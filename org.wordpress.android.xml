    <component-filter name="org.wordpress.android/.ui.notifications.receivers.NotificationsPendingDraftsReceiver" />
    <component-filter name="org.wordpress.android/.ui.stats.StatsWidgetProvider" />
    <component-filter name="org.wordpress.android/com.google.android.gms.auth.api.signin.RevocationBoundService" />
    <component-filter name="org.wordpress.android/com.google.android.gms.gcm.GcmReceiver" />
